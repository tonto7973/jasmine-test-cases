{
  "name": "jasmine-test-cases",
  "version": "1.0.0",
  "description": "Parametrized test cases for Jasmine",
  "author": "Ondrej Urik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tonto7973/jasmine-test-cases/issues"
  },
  "homepage": "https://github.com/tonto7973/jasmine-test-cases",
  "keywords": [
    "parametrized",
    "test",
    "case",
    "jasmine",
    "tests"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonto7973/jasmine-test-cases.git"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dist": "npm run clean && npm run lint && npm run test && npm run build && node dist.js",
    "build": "tsc -p tsconfig.build.json --listEmittedFiles true --diagnostics true",
    "clean": "rimraf dist && rimraf .nyc_output && rimraf coverage",
    "lint": "tslint -p tsconfig.json",
    "test": "nyc jasmine-ts --fail-fast=false --random=false --helper=jasmine.conf.js src/**/*.spec.ts",
    "tdd": "nodemon --watch src -e ts --exec \"jasmine-ts --fail-fast=false --random=false --helper=jasmine.conf.js src/**/*.spec.ts\"",
    "e2e:node": "jasmine-ts --random=false --helper=jasmine.conf.js e2e/test/**/*.spec.ts",
    "e2e:browser": "karma start e2e/karma.conf.js"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.8",
    "@types/jasmine-expect": "3.8.1",
    "@types/mock-require": "2.0.0",
    "@types/node": "10.3.4",
    "awesome-typescript-loader": "5.0.0",
    "chalk": "2.4.1",
    "cross-env": "5.1.6",
    "jasmine": "3.1.0",
    "jasmine-console-reporter": "3.0.2",
    "jasmine-expect": "3.8.3",
    "jasmine-ts": "0.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "3.0.0",
    "mock-require": "3.0.2",
    "nodemon": "1.17.5",
    "nyc": "12.0.2",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.6",
    "ts-node": "6.1.0",
    "tslint": "5.10.0",
    "typescript": "2.8.3",
    "webpack": "4.12.0"
  },
  "peerDependencies": {
    "jasmine": "^3.1.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "./dist/**/*",
      "./coverage/**/*",
      "./.nyc_output/**/*",
      "**/*.d.ts",
      "**/*.spec.ts"
    ],
    "include": [
      "./src/**/*"
    ],
    "all": true,
    "reporter": [
      "text",
      "html"
    ]
  }
}
